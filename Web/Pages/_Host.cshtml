@page "/"
@namespace Web.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Web</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/blazing berry/bootstrap.min.css">
    <link href="css/site.css" rel="stylesheet" />

    <link href="_content/DevExpress.Blazor/dx-blazor.css" rel="stylesheet" />
    <link href="~/Long_Custom/Long.css" rel="stylesheet"/>
    <script type="text/javascript" src="site.js"></script>
</head>
<body>

    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script src="~/Long_Custom/Long.js"></script>
    <script src="~/Long_Custom/Excel.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD1AMyEx9eRaZpeHPkY0subn3uA_Fa69fE&callback=initMap&libraries=&v=weekly" async></script>
</body>
</html>
